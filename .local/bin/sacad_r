#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'sacad==2.2.3','console_scripts','sacad_r'
__requires__ = 'sacad==2.2.3'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('sacad==2.2.3', 'console_scripts', 'sacad_r')()
    )
